<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="40"
             x:Class="Launcher.Forms.FormTemplates.TextInput">
    <DockPanel Height="40">
        <Label Margin="0,0,10,0" VerticalAlignment="Center" DockPanel.Dock="Left" Name="Label">Label:</Label>
        <TextBox VerticalAlignment="Center" Watermark="Enter text here" Name="TextBox" />
    </DockPanel>
</UserControl>
